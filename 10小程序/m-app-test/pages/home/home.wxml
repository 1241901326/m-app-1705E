    <text wx:for="{{navList}}" wx:key="{{index}}">{{item.title}}</text>
  </view>
  <view class="m-content">
    <scroll-view  scroll-y="{{true}}" class="m-content-scroll">
      <view>content</view> 
      <view>content</view> 
      <view>content</view> 
      <view>content</view> 
      <view>content</view> 
      <view>content</view> 
      <view>content</view> 
      <view>content</view> 
      <view>content</view> 
    </scroll-view>
  </view>
</scroll-view>